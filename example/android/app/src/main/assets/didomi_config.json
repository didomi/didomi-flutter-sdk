{
    "app": {
        "name": "Didomi x Flutter",
        "vendors": {
            "iab": {
                "all": true,
                "enabled": true,
                "exclude": [],
                "include": [],
                "version": 2
            },
            "didomi": [
                "amazon",
                "twitter",
                "salesforce",
                "google",
                "facebook"
            ]
        },
        "privacyPolicyURL": "https://didomi.io/",
        "gdprAppliesGlobally": true,
        "gdprAppliesWhenUnknown": false,
        "consentDuration": 31622400
    },
    "user": {
        "bots": {
            "consentRequired": false
        }
